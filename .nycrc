{
  "cache": false,
  "check-coverage": true,
  "lines": 100,
  "statements": 100,
  "functions": 100,
  "branches": 100,
  "extension": [
    ".js"
  ],
  "include": [
    "**/*.js"
  ],
  "exclude": [
    "coverage/**",
    "reports/**",
    "node_modules/**",
    "test/**",
    "src/index.js",
    ".eslintrc.js"
  ],
  "sourceMap": true,
  "reporter": [
    "html",
    "text",
    "text-summary"
  ],
  "all": true,
  "instrument": true
}